ass=\"schema-faq-section\"><strong class=\"schema-faq-question\">\u0645\u062a\u0649 \u064a\u0642\u062f\u0645 \u0627\u0644\u0627\u0633\u062a\u0626\u0646\u0627\u0641\u061f<\/strong>\r\n<p class=\"schema-faq-answer\">\u0641\u064a \u0627\u0644\u0635\u0644\u062d<br \/>\u0645\u064a\u0639\u0627\u062f \u0627\u0644\u0627\u0633\u062a\u0626\u0646\u0627\u0641 \u0641\u064a \u0627\u0644\u0623\u062d\u0643\u0627\u0645 \u0627\u0644\u0635\u0644\u062d\u064a\u0629 \u0639\u0634\u0631\u0629 \u0623\u064a\u0627\u0645 \u062a\u0628\u062f\u0623 \u0645\u0646 \u0627\u0644\u064a\u0648\u0645 \u0627\u0644\u062a\u0627\u0644\u064a \u0644\u062a\u0641\u0647\u0645 \u0627\u0644\u062d\u064f\u0643\u0645 \u0625\u0630\u0627 \u0643\u0627\u0646 \u0648\u062c\u0627\u0647\u064a\u0627\u064b\u060c \u0623\u0648 \u0645\u0646 \u062a\u0627\u0631\u064a\u062e \u062a\u0628\u0644\u064a\u063a\u0647 \u0625\u0630\u0627 \u0644\u0645 \u064a\u0643\u0646 \u0648\u062c\u0627\u0647\u064a\u0627\u064b.<br \/><strong>\u0641\u064a \u0627\u0644\u0628\u062f\u0627\u064a\u0629<\/strong><br \/>\u0648\u062a\u0643\u0648\u0646 \u0645\u062f\u0629 \u0627\u0644\u0637\u0639\u0646 \u0628\u0627\u0644\u0627\u0633\u062a\u0626\u0646\u0627\u0641 \u062b\u0644\u0627\u062b\u064a\u0646 \u064a\u0648\u0645\u0627\u064b \u0641\u064a \u0627\u0644\u0623\u062d\u0643\u0627\u0645 \u0627\u0644\u0645\u0646\u0647\u064a\u0629 \u0644\u0644\u062e\u0635\u0648\u0645\u0629 \u0645\u0627 \u0644\u0645 \u064a\u0646\u0635 \u0642\u0627\u0646\u0648\u0646 \u062e\u0627\u0635 \u0639\u0644\u0649 \u062e\u0644\u0627\u0641 \u0630\u0644\u0643\u060c \u0648\u062a\u0643\u0648\u0646 \u0645\u062f\u0629 \u0627\u0644\u0637\u0639\u0646 \u0639\u0634\u0631\u0629 \u0623\u064a\u0627\u0645 \u0641\u064a \u0627\u0644\u0642\u0631\u0627\u0631\u0627\u062a \u0627\u0644\u0642\u0627\u0628\u0644\u0629 \u0644\u0644\u0637\u0639\u0646.<br \/>\u0641\u064a \u0627\u0644\u0642\u0636\u0627\u064a\u0627 \u0627\u0644\u062c\u0632\u0627\u0626\u064a\u0629<br \/>\u0627\u0633\u062a\u0626\u0646\u0627\u0641 \u0627\u0644\u062d\u0