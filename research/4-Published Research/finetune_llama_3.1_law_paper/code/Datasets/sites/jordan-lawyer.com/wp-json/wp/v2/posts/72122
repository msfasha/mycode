u0639\u0646\u0647 \u0623\u0648 \u0648\u0635\u064a\u0627 \u0623\u0648 \u0642\u064a\u0645\u0627 \u0639\u0644\u064a\u0647 \u0645\u0635\u0644\u062d\u0629 \u0641\u064a \u0627\u0644\u062f\u0639\u0648\u0649 \u0627\u0644\u0642\u0627\u0626\u0645\u0629.<br\/>5. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0628\u064a\u0646\u0647 \u0648\u0628\u064a\u0646 \u0623\u062d\u062f \u0642\u0636\u0627\u0629 \u0627\u0644\u0647\u064a\u0626\u0629 \u0635\u0644\u0629 \u0642\u0631\u0627\u0628\u0629 \u0623\u0648 \u0645\u0635\u0627\u0647\u0631\u0629 \u0644\u0644\u062f\u0631\u062c\u0629 \u0627\u0644\u0631\u0627\u0628\u0639\u0629 \u0623\u0648 \u0643\u0627\u0646 \u0628\u064a\u0646\u0647 \u0648\u0628\u064a\u0646 \u0627\u0644\u0645\u062f\u0627\u0641\u0639 \u0639\u0646 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0635\u0644\u0629 \u0642\u0631\u0627\u0628\u0629 \u0623\u0648 \u0645\u0635\u0627\u0647\u0631\u0629 \u0644\u0644\u062f\u0631\u062c\u0629 \u0627\u0644\u062b\u0627\u0646\u064a\u0629.<br\/>6. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0642\u062f \u0623\u0641\u062a\u0649 \u0623\u0648 \u062a\u0631\u0627\u0641\u0639 \u0639\u0646 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0641\u064a \u0627\u0644\u062f\u0639\u0648\u0649 \u0648\u0644\u0648 \u0643\u0627\u0646 \u0630\u0644\u0643 \u0642\u0628\u0644 \u0627\u0634\u062a\u063a\u0627\u0644\u0647 \u0641\u064a \u0627\u0644\u0642\u0636\u0627\u0621\u060c \u0623\u0648 \u0643\u0627\u0646 \u0642\u062f \u0633\u0628\u0642 \u0644\u0647 \u0646\u0638\u0631\u0647\u0627 \u0642\u0627\u0636\u064a\u0627 \u0623\u0648 \u062e\u0628\u064a\u0631\u0627 \u0623\u0648 \u0645\u062d\u0643\u0645\u0627 \u0623\u0648 \u0643\u0627\u0646 \u0642\u062f \u0623\u062f\u0649 \u0634\u0647\u0627\u062f\u0629 \u0641\u064a\u0647\u0627.<br\/>7. \u0625\u0630\u0627 \u0631\u0641\u0639 \u062f\u0639\u0648\u0649 \u062a\u0639\u0648\u064a\u0636 \u0639\u0644\u0649 \u0637\u0627\u0644\u0628 \u0627\u0644\u0631\u062f \u0623\u0648 \u0642\u062f\u0645 \u0636\u062f\u0647 \u0628\u0644\u0627\u063a\u0627 \u0644\u062c\u0647\u0629 \u0627\u0644\u0627\u062e\u062a\u0635\u0627\u0635.<br\/>\u0648\u0625\u0636\u0627\u0641\u0629 \u0625\u0644\u0649 \u0623\u0633\u0628\u0627\u0628 \u0639\u062f\u0645 \u0627\u0644\u0635\u0644\u0627\u062d\u064a\u0629 \u0627\u0644\u062a\u064a \u062a\u0646\u0627\u0648\u0644\u062a\u0647\u0627 \u0627\u0644\u0645\u0627\u062f\u0629 \u0627\u0644\u0633\u0627\u0628\u0642\u0629 \u0641\u0625\u0646 \u0647\u0646\u0627\u0643 \u0623\u0633\u0628\u0627\u0628\u064b\u0627 \u0623\u062e\u0631\u0649 \u0623\u0642\u0644 \u062a\u0623\u062b\u064a\u0631\u064b\u0627 \u0639\u0644\u0649 \u062d\u064a\u0627\u062f\u0647 \u0648\u064a\u064f\u0637\u0644\u0642 \u0639\u0644\u064a\u0647\u0627 \u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u0631\u062f \u0648\u0642\u062f \u062a\u0646\u0627\u0648\u0644\u062a\u0647\u0627 (\u0627\u0644\u0645\u0627\u062f\u0629 134) \u0645\u0646 \u0642\u0627\u0646\u0648\u0646 \u0623\u0635\u0648\u0644 \u0627\u0644\u0645\u062d\u0627\u0643\u0645\u0627\u062a \u0627\u0644\u0623\u0631\u062f\u0646\u064a\u0629 \u0648\u0647\u064a:<br\/>\u064a\u062c\u0648\u0632 \u0631\u062f \u0627\u0644\u0642\u0627\u0636\u064a \u0644\u0623\u062d\u062f \u0627\u0644\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u062a\u0627\u0644\u064a\u0629:<br\/>1. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0644\u0647 \u0623\u0648 \u0644\u0632\u0648\u062c\u0647 \u062f\u0639\u0648\u0649 \u0645\u0645\u0627\u062b\u0644\u0629 \u0644\u0644\u062f\u0639\u0648\u0649 \u0627\u0644\u062a\u064a \u064a\u0646\u0638\u0631\u0647\u0627 \u0623\u0648 \u0625\u0630\u0627 \u062c\u062f\u062a \u0644\u0623\u062d\u062f\u0647\u0645\u0627 \u062e\u0635\u0648\u0645\u0629 \u0645\u0639 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0623\u0648 \u0645\u0639 \u0632\u0648\u062c\u0647 \u0628\u0639\u062f \u0642\u064a\u0627\u0645 \u0627\u0644\u062f\u0639\u0648\u0649 \u0627\u0644\u0645\u0637\u0631\u0648\u062d\u0629 \u0639\u0644\u0649 \u0627\u0644\u0642\u0627\u0636\u064a \u0645\u0627 \u0644\u0645 \u062a\u0643\u0646 \u0647\u0630\u0647 \u0627\u0644\u062f\u0639\u0648\u0649 \u0642\u062f \u0623\u0642\u064a\u0645\u062a \u0628\u0642\u0635\u062f \u0631\u062f\u0647 \u0639\u0646 \u0646\u0638\u0631 \u0627\u0644\u062f\u0639\u0648\u0649 \u0627\u0644\u0645\u0637\u0631\u0648\u062d\u0629 \u0639\u0644\u064a\u0647.<br\/>2. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0644\u0645\u0637\u0644\u0642\u062a\u0647 \u0627\u0644\u062a\u064a \u0644\u0647 \u0645\u0646\u0647\u0627 \u0648\u0644\u062f \u0623\u0648 \u0644\u0623\u062d\u062f \u0623\u0642\u0627\u0631\u0628\u0647 \u0623\u0648 \u0623\u0635\u0647\u0627\u0631\u0647 \u0639\u0644\u0649 \u0639\u0645\u0648\u062f \u0627\u0644\u0646\u0633\u0628 \u062e\u0635\u0648\u0645\u0629 \u0642\u0627\u0626\u0645\u0629 \u0623\u0645\u0627\u0645 \u0627\u0644\u0642\u0636\u0627\u0621 \u0645\u0639 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0641\u064a \u0627\u0644\u062f\u0639\u0648\u0649 \u0623\u0648 \u0645\u0639 \u0632\u0648\u062c\u0647 \u0645\u0627 \u0644\u0645 \u062a\u0643\u0646 \u0647\u0630\u0647 \u0627\u0644\u062e\u0635\u0648\u0645\u0629 \u0642\u062f \u0623\u0642\u064a\u0645\u062a \u0628\u0639\u062f \u0642\u064a\u0627\u0645 \u0627\u0644\u062f\u0639\u0648\u0649 \u0627\u0644\u0645\u0637\u0631\u0648\u062d\u0629 \u0639\u0644\u0649 \u0627\u0644\u0642\u0627\u0636\u064a \u0628\u0642\u0635\u062f \u0631\u062f\u0647.<br\/>3. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u064a\u0639\u0645\u0644 \u0639\u0646\u062f\u0647.<br\/>4. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0642\u062f \u0627\u0639\u062a\u0627\u062f \u0645\u0633\u0627\u0643\u0646\u0629 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0623\u0648 \u0643\u0627\u0646 \u0642\u062f \u062a\u0644\u0642\u0649 \u0645\u0646\u0647 \u0647\u062f\u064a\u0629 \u0642\u0628\u064a\u0644 \u0631\u0641\u0639 \u0627\u0644\u062f\u0639\u0648\u0649 \u0623\u0648 \u0628\u0639\u062f\u0647.<br\/>5. \u0625\u0630\u0627 \u0643\u0627\u0646 \u0628\u064a\u0646\u0647 \u0648\u0628\u064a\u0646 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645 \u0639\u062f\u0627\u0648\u0629 \u0623\u0648 \u0645\u0648\u062f\u0629 \u064a\u0631\u062c\u062d \u0645\u0639\u0647\u0627 \u0639\u062f\u0645 \u0627\u0633\u062a\u0637\u0627\u0639\u062a\u0647 \u0627\u0644\u062d\u0643\u0645 \u0628\u063a\u064a\u0631 \u0645\u064a\u0644.<br\/>\u0648\u0642\u062f \u0635\u0631\u062d\u062a \u0628\u0639\u0636 \u0627\u0644\u062a\u0634\u0631\u064a\u0639\u0627\u062a \u0623\u0646 \u0623\u0633\u0628\u0627\u0628 \u0631\u062f \u0627\u0644\u0642\u0636\u0627\u0629 \u0647\u064a \u0646\u0641\u0633\u0647\u0627 \u0623\u0633\u0628\u0627\u0628 \u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645\u064a\u0646 \u0645\u062b\u0644 \u0642\u0627\u0646\u0648\u0646 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u064a\u0645\u0646\u064a \u0648\u0642\u0627\u0646\u0648\u0646 \u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0645\u062f\u0646\u064a\u0629 \u0627\u0644\u0625\u0645\u0627\u0631\u0627\u062a\u064a\u060c \u0648\u064a\u062c\u0648\u0632 \u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645\u064a\u0646 \u0644\u0644\u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u0648\u0627\u0631\u062f\u0629 \u0641\u064a \u0642\u0627\u0646\u0648\u0646 \u0623\u0635\u0648\u0644 \u0627\u0644\u0645\u062d\u0627\u0643\u0645\u0627\u062a \u0627\u0644\u0645\u062f\u0646\u064a\u0629 \u0648\u0644\u0623\u064a \u0623\u0633\u0628\u0627\u0628 \u0623\u062e\u0631\u0649 \u064a\u0631\u0627\u0647\u0627 \u0637\u0627\u0644\u0628 \u0627\u0644\u0631\u062f\u060c \u0625\u0630 \u0623\u0646 \u0646\u0635 \u0627\u0644\u0641\u0642\u0631\u0629 \u0627\u0644\u0623\u0648\u0644\u0649 \u0645\u0646 (\u0627\u0644\u0645\u0627\u062f\u0629 17) \u0645\u0646 \u0642\u0627\u0646\u0648\u0646 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u0623\u0631\u062f\u0646\u064a \u0642\u062f \u062c\u0627\u0621 \u0645\u0637\u0644\u0642\u064b\u0627 \u062f\u0648\u0646 \u0642\u064a\u062f.<a href=\\\"#_ftn1\\\">[1]<\/a>\",\"inLanguage\":\"ar\"},\"inLanguage\":\"ar\"},{\"@type\":\"Question\",\"@id