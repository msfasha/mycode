trong class=\"schema-faq-question\">5-\u00a0\u00a0 \u0645\u0627 \u0647\u064a \u0625\u062c\u0631\u0627\u0621\u0627\u062a \u062a\u0642\u062f\u064a\u0645 \u0637\u0644\u0628 \u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645\u061f<\/strong> <p class=\"schema-faq-answer\">\u0628\u0627\u0644\u0631\u062c\u0648\u0639 \u0644\u0642\u0627\u0646\u0648\u0646 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u0623\u0631\u062f\u0646\u064a \u0646\u062c\u062f \u0623\u0646 (\u0627\u0644\u0645\u0627\u062f\u0629 18) \u0642\u062f \u0646\u0635\u062a \u0639\u0644\u0649 \u0627\u0644\u0636\u0648\u0627\u0628\u0637 \u0627\u0644\u0625\u062c\u0631\u0627\u0626\u064a\u0629 \u0644\u0637\u0644\u0628 \u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645 \u0648\u0627\u0644\u0622\u062b\u0627\u0631 \u0627\u0644\u0645\u062a\u0631\u062a\u0628\u0629 \u0639\u0644\u0649 \u062a\u0642\u062f\u064a\u0645\u0647 \u0648\u0627\u0644\u0641\u0635\u0644 \u0641\u064a\u0647 \u0641\u0646\u062c\u062f \u0623\u0646\u0647\u0627 \u0646\u0635\u062a \u0639\u0644\u0649 \u0645\u0627 \u064a\u0644\u064a:<br \/>\u0623- \u064a\u0642\u062f\u0645 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0643\u062a\u0627\u0628\u0629 \u0625\u0644\u0649 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0645\u0628\u064a\u0646\u0627 \u0641\u064a\u0647 \u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u0631\u062f\u060c \u0645\u0639 \u0628\u064a\u0646\u0627\u062a\u0647 \u0641\u064a \u0627\u0644\u0637\u0644\u0628\u060c \u062e\u0644\u0627\u0644 \u062e\u0645\u0633\u0629 \u0639\u0634\u0631 \u064a\u0648\u0645\u0627 \u0645\u0646 \u062a\u0627\u0631\u064a\u062e \u0639\u0644\u0645 \u0637\u0627\u0644\u0628 \u0627\u0644\u0631\u062f \u0628\u062a\u0634\u0643\u064a\u0644 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0623\u0648 \u0628\u0627\u0644\u0638\u0631\u0648\u0641 \u0627\u0644\u0645\u0628\u0631\u0631\u0629 \u0644\u0644\u0631\u062f\u060c \u0641\u0627\u0630\u0627 \u0644\u0645 \u064a\u062a\u0646\u062d\u064e\u0651 \u0627\u0644\u0645\u062d\u0643\u0645 \u0627\u0644\u0645\u0637\u0644\u0648\u0628 \u0631\u062f\u0647 \u0641\u0639\u0644\u064a\u0647 \u0623\u0646 \u064a\u0642\u062f\u0645 \u062c\u0648\u0627\u0628\u0647 \u0639\u0644\u0649 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0648\u0627\u0644\u0628\u064a\u0646\u0627\u062a \u062e\u0644\u0627\u0644 \u062e\u0645\u0633\u0629 \u0639\u0634\u0631 \u064a\u0648\u0645\u0627 \u0645\u0646 \u062a\u0627\u0631\u064a\u062e \u062a\u0642\u062f\u064a\u0645 \u0627\u0644\u0637\u0644\u0628\u060c \u0648\u0639\u0644\u0649 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0641\u064a \u0647\u0630\u0647 \u0627\u0644\u062d\u0627\u0644\u0629 \u0628\u0646\u0627\u0621 \u0639\u0644\u0649 \u0637\u0644\u0628 \u0637\u0627\u0644\u0628 \u0627\u0644\u0631\u062f \u0625\u062d\u0627\u0644\u0629 \u0627\u0644\u0637\u0644\u0628 \u0645\u0639 \u062c\u0648\u0627\u0628 \u0627\u0644\u0645\u062d\u0643\u0645 \u0627\u0644\u0645\u0637\u0644\u0648\u0628 \u0631\u062f\u0647 \u0625\u0646 \u0648\u062c\u062f \u0625\u0644\u0649 \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629 \u0644\u0644\u0628\u062a\u0651 \u0641\u064a\u0647.<br \/>\u0628- \u064a\u0646\u0638\u0631 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0645\u0646 \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629 \u062a\u062f\u0642\u064a\u0642\u0627\u064b \u0645\u0627 \u0644\u0645 \u062a\u0642\u0631\u0631 \u062e\u0644\u0627\u0641 \u0630\u0644\u0643 \u0648\u0639\u0644\u064a\u0647\u0627 \u0623\u0646 \u062a\u0641\u0635\u0644 \u0641\u064a\u0647 \u062e\u0644\u0627\u0644 \u062b\u0644\u0627\u062b\u064a\u0646 \u064a\u0648\u0645\u0627\u064b \u0645\u0646 \u062a\u0627\u0631\u064a\u062e \u0648\u0631\u0648\u062f\u0647 \u0644\u0642\u0644\u0645\u0647\u0627 \u0648\u064a\u0643\u0648\u0646 \u0642\u0631\u0627\u0631\u0647\u0627 \u063a\u064a\u0631 \u0642\u0627\u0628\u0644 \u0644\u0644\u0637\u0639\u0646 \u0641\u064a\u0647 \u0628\u0623\u064a \u0637\u0631\u064a\u0642 \u0645\u0646\u00a0\u0637\u0631\u0642 \u0627\u0644\u0637\u0639\u0646.<br \/>\u062c- \u0644\u0627 \u064a\u0642\u0628\u0644 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0645\u0645\u0646 \u0633\u0628\u0642 \u0644\u0647 \u062a\u0642\u062f\u064a\u0645 \u0637\u0644\u0628 \u0628\u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645 \u0646\u0641\u0633\u0647 \u0641\u064a \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0630\u0627\u062a\u0647 \u0648\u0644\u0644\u0633\u0628\u0628 \u0630\u0627\u062a\u0647.<br \/>\u062f- \u0644\u0627 \u064a\u062a\u0631\u062a\u0628 \u0639\u0644\u0649 \u062a\u0642\u062f\u064a\u0645 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0648\u0642\u0641 \u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u062a\u062d\u0643\u064a\u0645\u060c \u0648\u0625\u0630\u0627 \u062d\u0643\u0645 \u0628\u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645 \u062a\u0639\u062a\u0628\u0631 \u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u062a\u064a \u0634\u0627\u0631\u0643 \u0641\u064a\u0647\u0627 \u0628\u0645\u0627 \u0641\u064a\u0647\u0627 \u062d\u0643\u0645 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u0646\u0647\u0627\u0626\u064a \u0643\u0623\u0646 \u0644\u0645 \u062a\u0643\u0646 \u0648\u0644\u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0628\u062a\u0634\u0643\u064a\u0644\u0647\u0627 \u0627\u0644\u062c\u062f\u064a\u062f \u0627\u0639\u062a\u0645\u0627\u062f \u0623\u064a \u0645\u0646 \u0627\u0644\u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u0633\u0627\u0628\u0642\u0629\u060c \u0639\u0644\u0649 \u0623\u0646 \u064a\u0628\u0642\u0649 \u062a\u0639\u064a\u064a\u0646 \u0631\u0626\u064a\u0633 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u0630\u064a \u0634\u0627\u0631\u0643 \u0641\u064a \u0627\u062e\u062a\u064a\u0627\u0631\u0647 \u0627\u0644\u0645\u062d\u0643\u0645 \u0627\u0644\u0645\u062d\u0643\u0648\u0645 \u0628\u0631\u062f\u0647 \u0635\u062d\u064a\u062d\u0627\u064b.<br \/>\u0648\u064a\u064c\u0641\u0647\u0645 \u0645\u0646 \u0627\u0644\u0645\u0627\u062f\u0629 \u0633\u0627\u0644\u0641\u0629 \u0627\u0644\u0630\u0643\u0631 \u0623\u0646 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u064a\u062a\u0639\u064a\u0646 \u0623\u0646 \u064a\u064f\u0642\u062f\u0645 \u0645\u0643\u062a\u0648\u0628\u064b\u0627 \u0625\u0644\u0649 \u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0645\u0628\u064a\u0646\u064b\u0627 \u0641\u064a\u0647 \u0623\u0633\u0628\u0627\u0628 \u0627\u0644\u0631\u062f\u060c \u0648\u0623\u0646 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0644\u0627\u0628\u062f \u0623\u0646 \u064a\u064f\u0642\u062f\u0645 \u0645\u0646 \u0623\u062d\u062f \u0627\u0644\u062e\u0635\u0648\u0645\u060c \u0648\u0642\u062f \u062d\u062f\u062f\u062a \u0627\u0644\u0645\u062f\u0629 \u0627\u0644\u0642\u0627\u0646\u0648\u0646\u064a\u0629 \u0648\u0627\u0644\u0638\u0631\u0641 \u0627\u0644\u0642\u0627\u0646\u0648\u0646\u064a \u0644\u062a\u0642\u062f\u064a\u0645 \u0627\u0644\u0637\u0644\u0628\u060c \u0648\u062d\u062f\u062f\u062a \u0627\u0644\u0625\u062c\u0631\u0627\u0621 \u0627\u0644\u0645\u0637\u0644\u0648\u0628 \u0627\u0644\u0642\u064a\u0627\u0645 \u0628\u0647 \u0641\u064a \u062d\u0627\u0644 \u0631\u0641\u0636 \u0627\u0644\u0645\u062d\u0643\u0645 \u0627\u0644\u0645\u0637\u0644\u0648\u0628 \u0631\u062f\u0647 \u0627\u0644\u062a\u0646\u062d\u064a\u060c \u0648\u0623\u0646 \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629 \u0628\u0627\u0644\u0641\u0635\u0644 \u0641\u064a \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u064a\u0643\u0648\u0646 \u0642\u0631\u0627\u0631\u0647\u0627 \u063a\u064a\u0631 \u0642\u0627\u0628\u0644 \u0644\u0644\u0637\u0639\u0646 \u0641\u064a\u0647 \u0648\u0639\u0644\u064a\u0647\u0627 \u0623\u0646 \u062a\u0641\u0635\u0644 \u0641\u064a \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u062e\u0644\u0627\u0644 \u0645\u062f\u0629 \u0645\u0639\u064a\u0646\u0629.<br \/>\u0639\u0644\u0649 \u0623\u0646\u0647 \u0644\u0627 \u062a\u062a\u0648\u0642\u0641 \u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0628\u0645\u062c\u0631\u062f \u062a\u0642\u062f\u064a\u0645 \u0637\u0644\u0628 \u0644\u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645\u060c \u0648\u0644\u0643\u0646 \u0625\u0630\u0627 \u062d\u0643\u0645\u062a \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0628\u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645 \u0641\u0625\u0646 \u0625\u062c\u0631\u0627\u0621\u0627\u062a \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0627\u0644\u0633\u0627\u0628\u0642\u0629 \u062a\u0635\u0628\u062d \u0648\u0627\u0644\u0639\u062f\u0645 \u0633\u0648\u0627\u0621.<\/p> <\/div> <div class=\"schema-faq-section\" id=\"faq-question-1635971351261\"><strong class=\"schema-faq-question\">6-\u00a0\u00a0 \u0647\u0644 \u064a\u062c\u0648\u0632 \u0644\u0637\u0627\u0644\u0628 \u0627\u0644\u0631\u062f \u0623\u0646 \u064a\u064f\u0642\u062f\u0645 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0645\u0628\u0627\u0634\u0631\u0629 \u0625\u0644\u0649 \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629\u061f<\/strong> <p class=\"schema-faq-answer\">\u0644\u0627 \u064a\u062c\u0648\u0632 \u0630\u0644\u0643\u060c \u0644\u0623\u0646 \u0630\u0644\u0643 \u064a\u062e\u0627\u0644\u0641 \u0627\u0644\u0637\u0631\u064a\u0642 \u0627\u0644\u0630\u064a \u0631\u0633\u0645\u0647 \u0627\u0644\u0642\u0627\u0646\u0648\u0646\u060c \u0648\u064a\u062a\u0631\u062a\u0628 \u0639\u0644\u0649 \u062a\u0642\u062f\u064a\u0645 \u0627\u0644\u0645\u062d\u062a\u0643\u0645 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0645\u0628\u0627\u0634\u0631\u0629 \u0644\u0644\u0645\u062d\u0643\u0645\u0629 \u0631\u062f \u0637\u0644\u0628\u0647.<\/p> <\/div> <div class=\"schema-faq-section\" id=\"faq-question-1635971361825\"><strong class=\"schema-faq-question\">7-\u00a0\u00a0 \u0645\u0627 \u062d\u0643\u0645\u0629 \u0627\u0644\u0642\u0627\u0646\u0648\u0646 \u0645\u0646 \u0636\u0631\u0648\u0631\u0629 \u062a\u0642\u062f\u064a\u0645 \u0637\u0644\u0628 \u0627\u0644\u0631\u062f \u0644\u0644\u0645\u062d\u0643\u0645 \u0623\u0648 \u0644\u0647\u064a\u0626\u0629 \u0627\u0644\u062a\u062d\u0643\u064a\u0645 \u0642\u0628\u0644 \u062a\u0642\u062f\u064a\u0645\u0647 \u0644\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629\u061f<\/strong> <p class=\"schema-faq-answer\">\u0627\u0644\u0633\u0628\u0628 \u0641\u064a \u0630\u0644\u0643 \u0647\u0648 \u0623\u062a\u0627\u062d\u0647 \u0627\u0644\u0641\u0631\u0635\u0629 \u0644\u0644\u0645\u062d\u0643\u0645 \u0644\u064a\u0642\u0648\u0645 \u0628\u0627\u0644\u062a\u0646\u062d\u064a \u0628\u0646\u0641\u0633\u0647 \u062f\u0648\u0646 \u0623\u0646 \u064a\u0645\u0633 \u0630\u0644\u0643 \u0643\u0631\u0627\u0645\u062a\u0647 \u0648\u0633\u0645\u0639\u062a\u0647.<\/p> <\/div> <div class=\"schema-faq-section\" id=\"faq-question-1635971380759\"><strong class=\"schema-faq-question\">8-\u00a0\u00a0 \u0645\u0627 \u0647\u064a \u0627\u0644\u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0645\u062e\u062a\u0635\u0629 \u0627\u0644\u062a\u064a \u064a\u064f\u0642\u062f\u0645 \u0625\u0644\u064a\u0647\u0627 \u0643\u0627\u0641\u0629 \u0637\u0644\u0628\u0627\u062a \u0631\u062f \u0627\u0644\u0645\u062d\u0643\u0645\u064a\u0646\u061f<\/strong> <p class=\"schema-faq-answer\">\u062a\u062e\u062a\u0635 \u0645\u062d\u0643\u0645\u0629 \u0627\u0644\u0627\u0633\u062a\u0626\u0646\u0627\u0641 \u0628\u0627\u0644\u0641\u0635\u0644 \u0641\u064a \u0627\u0644\u0637\u0644\u0628 \u0627\u0644\u0645\u064f\u0642\u062f\u0645 \u0625\u0644\u064a\u0647\u0627 \u062a\u062f\u0642\u064a\u0642\u064b\u0627 \u0625\u0630\u0627 \u0644\u0645 \u064a\u0643\u0646 \u0644\u0647\u0627 \u0631\u0623\u064a \u0622\u062e\u0631 \u0623\u0648 \u0642\u0631\u0627\u0631 \u0622\u062e\u0631\u060c \u0648\u062a\u0643\u0648\u0646 \u0645\u0644\u0632\u0645\u0629 \u0623\u0646 \u062a\u0641\u0635\u0644 \u0641\u064a\u0647 \u062e\u0644\u0627\u0644 \u0645\u062f\u0629 \u0642\u062f\u0631\u0647\u0627 \u0627\u0